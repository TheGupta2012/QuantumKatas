   "source": [
    "operation RandNum() : Result {\n",
    "\n",
    "    use q = Qubit();\n",
    "    H(q);\n",
    "    for \n",
    "    let r = M(q);\n",
    "    Message(\"Got a random qubit state!\");\n",
    "    // reset the qubit \n",
    "    Reset(q);\n",
    "    return r;\n",
    "}"
   ]
